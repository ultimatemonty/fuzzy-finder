{
	"name": "fuzzy-finder",
	"displayName": "fuzzy-finder",
	"description": "quickly find and open files using cmd+t",
	"version": "0.0.1",
	"publisher": "ultimatemonty",
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:FuzzyFinder.fuzzySearch"
	],
	"main": "./out/src/extension",
	"contributes": {
        "commands": [
            {
                "command": "FuzzyFinder.fuzzySearch",
                "title": "Fuzzy Finder"
            }
        ],
        "keybindings": [
            {
                "command": "FuzzyFinder.fuzzySearch",
                "key": "cmd+t"
            },
			{
				"command": "FuzzyFinder.fuzzySearch",
				"key": "cmd+p"
			}
        ]
    },
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
	},
	"devDependencies": {
		"typescript": "^1.6.2",
		"vscode": "0.10.x"
	}
}